{
  "name": "hangout-chat-tester",
  "version": "0.0.2",
  "description": "A testing \"library\" exported as a single function to test locally whether your payload will will render in hangouts chat",
  "homepage": "",
  "author": {
    "name": "BaReinhard",
    "email": "brett@reinhards.us",
    "url": "https://brettreinhard.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "hangoutschat hangoutchat google-chat"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "nsp": "^2.8.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint . --fix",
    "precommit": "lint-staged",
    "test": "npm run test:coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "BaReinhard/hangout-chat-tester",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "Apache-2.0"
}